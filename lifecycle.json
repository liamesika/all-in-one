{
  "rule": [
    { "action": { "type": "Delete" }, "condition": { "age": 7,  "matchesPrefix": ["tmp/"] } },
    { "action": { "type": "Delete" }, "condition": { "age": 60, "matchesPrefix": ["jobs/"] } },
    { "action": { "type": "SetStorageClass", "storageClass": "NEARLINE" }, "condition": { "age": 90, "matchesPrefix": ["user-assets/"] } },
    { "action": { "type": "Delete" }, "condition": { "age": 365, "matchesPrefix": ["user-assets/"] } }
  ]
}
